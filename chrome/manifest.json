{
  "name": "cloudkeys",
  "description": "At the moment it's just a helper",
  "version": "0.1",
  "background": {
    "page": "background.html"
  },
  "content_scripts": [ {
     "js": [ "js/jquery-2.0.3.min.js", "js/copyToClipboard.js" ],
     "run_at": "document_end",
     "matches": [ "http://*.cloudkeyshq.com/*", "https://*.cloudkeyshq.com/*", "http://dev.mthie.com/cloudkeys/" ]
  } ],

  "web_accessible_resources": [ "js/jquery-2.0.3.min.map" ],
  "page_action": {},
  "permissions": [ "http://*.cloudkeyshq.com/*", "https://*.cloudkeyshq.com/*", "http://dev.mthie.com/cloudkeys/", "clipboardWrite" ],
  "manifest_version": 2
}
